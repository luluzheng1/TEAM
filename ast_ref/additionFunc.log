list<?> remove_int(list<?> l, int elem, bool all):
	list<?> retlist = [];
	int i = 0;
	int len = length(l);
	int remove_index = 0;
	if all:
		while i < len:
			if l[i] == elem:
				i = i + 1;
				continue;
			end
			retlist = append(retlist, l[i]);
			i = i + 1;
		end
	else:
		while i < len:
			if l[i] == elem:
				remove_index = i;
				break;
			end
			retlist = append(retlist, l[i]);
			i = i + 1;
		end
		if i != len:
			for index in remove_index + 1..len:
				retlist = append(retlist, l[index]);
			end
		end
	end
	return retlist;
end

