end

bool strcmp(string str1, string str2):
	if length(str1) != length(str2):
		return false;
	end
	int i = 0;
	while i < length(str1):
		char c1 = str1[i];
		char c2 = str2[i];
		if c1 != c2:
			return false;
		end
		i = i + 1;
	end
	return true;
end

